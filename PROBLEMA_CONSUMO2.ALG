Algoritmo "problema_consumo2"

Var

   distancia, combustivel, consumo : real


Inicio

      escreval ("Distancia percorrida: ")
      leia (distancia)
      escreval ("Combustivel gasto: ")
      leia (combustivel)
      
      consumo <- distancia / combustivel
      
      escreval ("Consumo medio = ", consumo)



Fimalgoritmo