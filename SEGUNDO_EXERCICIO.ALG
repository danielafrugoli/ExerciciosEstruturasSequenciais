Algoritmo "segundo_exercicio"

Var
    nome : caractere
    idade : inteiro
    altura : real
    sexo : caractere


Inicio
      escreval ("Digite seu nome:")
      leia (nome)
      escreval ("Digite sua idade:")
      leia (idade)
      escreval ("Digite sua altura:")
      leia (altura)
      escreval ("Digite seu sexo:")
      leia (sexo)

Fimalgoritmo